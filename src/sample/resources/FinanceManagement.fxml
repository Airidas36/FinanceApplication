<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.FinanceManagement">
   <children>
      <ListView fx:id="finList" prefHeight="401.0" prefWidth="328.0" />
      <Button fx:id="addFin" layoutX="400.0" layoutY="68.0" mnemonicParsing="false" onAction="#addFinance" prefHeight="43.0" prefWidth="118.0" text="Add" />
      <Button fx:id="updateFin" layoutX="400.0" layoutY="158.0" mnemonicParsing="false" onAction="#updateFinance" prefHeight="43.0" prefWidth="118.0" text="Update" />
      <Button fx:id="deleteFin" layoutX="400.0" layoutY="242.0" mnemonicParsing="false" onAction="#deleteFinance" prefHeight="43.0" prefWidth="118.0" text="Delete" />
   </children>
</AnchorPane>
