<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="163.0" prefWidth="400.0" style="-fx-background-color: #2b2830;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.Welcome">
   <children>
      <Label layoutX="45.0" layoutY="29.0" text="Welcome, please select an option." textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Button fx:id="signin" layoutX="252.0" layoutY="75.0" mnemonicParsing="false" onAction="#login" prefHeight="28.0" prefWidth="114.0" style="-fx-background-color: #453a8c; -fx-background-radius: 20;" text="Sign In" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="C059-Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="signup" layoutX="45.0" layoutY="75.0" mnemonicParsing="false" onAction="#register" prefHeight="28.0" prefWidth="114.0" style="-fx-background-color: #453a8c; -fx-background-radius: 20;" text="Sign Up" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="exitBtn" layoutX="143.0" layoutY="121.0" mnemonicParsing="false" onAction="#exit" prefHeight="28.0" prefWidth="114.0" style="-fx-background-color: #453a8c; -fx-background-radius: 20;" text="Exit" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
