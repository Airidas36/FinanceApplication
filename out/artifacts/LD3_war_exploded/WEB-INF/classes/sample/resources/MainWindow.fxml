<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="563.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.MainWindow">
   <children>
      <TreeView fx:id="categoryTree" prefHeight="400.0" prefWidth="330.0" />
      <Button fx:id="addBtn" layoutX="373.0" layoutY="73.0" mnemonicParsing="false" onAction="#addCategory" prefHeight="53.0" prefWidth="150.0" text="Add" />
      <Button fx:id="updateBtn" layoutX="373.0" layoutY="174.0" mnemonicParsing="false" onAction="#updateCategory" prefHeight="53.0" prefWidth="150.0" text="Update" />
      <Button fx:id="deleteBtn" layoutX="373.0" layoutY="271.0" mnemonicParsing="false" onAction="#deleteCategory" prefHeight="53.0" prefWidth="150.0" text="Delete" />
      <Label fx:id="financeLabel" layoutX="343.0" layoutY="145.0" prefHeight="102.0" prefWidth="218.0" text="Double-click on a category to add/update or delete its finances." visible="false" wrapText="true">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
